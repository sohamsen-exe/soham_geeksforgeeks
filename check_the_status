def checkstatus():
    a = int(input("Enter a number :"))
    b = int(input("Enter a number :"))
    flagg = False
    if (a > 0) and (b > 0):
        flagg = False
        print(flagg)
    elif (a < 0) and (b < 0):
        flagg = True
        print(flagg)
    else:
        flagg = False
        print(flagg)
checkstatus()
